{
    "presets": [
        ["env", {
            "targets": {
                "browsers": ["last 2 versions"],
                "ie": 11,
                "modules": false,
                "useBuiltIns": true
            }
        }]
    ],
    "plugins": [
        "syntax-dynamic-import",
        "transform-class-properties",
        "transform-async-to-generator",
        "transform-skate-flow-props"
    ]
}
